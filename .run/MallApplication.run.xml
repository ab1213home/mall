<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="MallApplication" type="SpringBootApplicationConfigurationType" factoryName="Spring Boot" nameIsGenerated="true">
    <option name="ACTIVE_PROFILES" value="application.properties" />
    <output_file path="\logs" is_save="true" />
    <module name="mall" />
    <option name="SPRING_BOOT_MAIN_CLASS" value="com.jiang.mall.MallApplication" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.jiang.mall.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>